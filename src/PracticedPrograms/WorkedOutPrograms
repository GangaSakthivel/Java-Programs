        // Autoboxing: Converting int to Integer
        int num = 10;
        Integer obj = num;
        System.out.println(obj);
        //unboxing
        Float obj2 = 3.9f;
        float num2 = obj2;
        System.out.println(num2);

        // Unboxing: Converting Integer to int
